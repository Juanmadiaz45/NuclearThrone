<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.nuclearthrone.control.MenuViewController">
   <children>
      <Label fx:id="MenuTitle" layoutX="262.0" layoutY="43.0" prefHeight="41.0" prefWidth="107.0" text="MENU" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="32.0" />
         </font>
      </Label>
      <Button fx:id="returnButton" onAction="#onReturnButton" layoutX="14.0" layoutY="18.0" mnemonicParsing="false" style="-fx-background-color: #e6f398;" text="Return" textAlignment="CENTER">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Button>
      <ImageView fitHeight="25.0" fitWidth="28.0" layoutX="300.0" layoutY="98.0" pickOnBounds="true" preserveRatio="true" smooth="false">
         <image>
            <Image url="@Images/EstrellaZoom.gif" />
         </image></ImageView>
      <Label fx:id="ChooseLevelText" layoutX="225.0" layoutY="136.0" text="Escoge un nivel para jugar" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Label>
      <Button onAction="#onFacilButton" layoutX="97.0" layoutY="188.0" mnemonicParsing="false" style="-fx-background-color: #e6f398;" text="Facil">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Button>
      <Button onAction="#onMedioButton" layoutX="287.0" layoutY="188.0" mnemonicParsing="false" style="-fx-background-color: #e6f398;" text="Medio">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font></Button>
      <Button onAction="#onDificilButton" layoutX="469.0" layoutY="188.0" mnemonicParsing="false" style="-fx-background-color: #e6f398;" text="Dificil">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>