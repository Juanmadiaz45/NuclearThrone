<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.nuclearthrone.control.GameViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Canvas fx:id="canvas" height="700.0" width="900.0" />
        <VBox alignment="BOTTOM_LEFT" spacing="10.0" style="-fx-background-color: transparent;">
            <Button mnemonicParsing="false" text="END GAME" onAction="#onReturnButton" />
        </VBox>
    </children>
</StackPane>